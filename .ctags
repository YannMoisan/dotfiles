--verbose=no
--recurse=yes
--tag-relative=yes
--langdef=scala
--langmap=scala:.scala
--regex-scala=/^[ \t]*class[ \t]*([a-zA-Z0-9_]+)/\1/c,classes/
--regex-scala=/^[ \t]*final[ \t]+class[ \t]*([a-zA-Z0-9_]+)/\1/c,classes/
--regex-scala=/^[ \t]*abstract[ \t]+class[ \t]*([a-zA-Z0-9_]+)/\1/c,classes/
--regex-scala=/^[ \t]*case[ \t]+class[ \t]*([a-zA-Z0-9_]+)/\1/r,cclasses/
--regex-scala=/^[ \t]*object[ \t]*([a-zA-Z0-9_]+)/\1/o,objects/
--regex-scala=/^[ \t]*case[ \t]+object[ \t]*([a-zA-Z0-9_]+)/\1/o,cobjects/
--regex-scala=/^[ \t]*trait[ \t]*([a-zA-Z0-9_]+)/\1/t,traits/
--regex-scala=/^[ \t]*sealed[ \t]+trait[ \t]*([a-zA-Z0-9_]+)/\1/t,traits/
--regex-scala=/^[ \t]*def[ \t]*([a-zA-Z0-9_=]+)[ \t]*.*[:=]/\1/m,methods/
--regex-scala=/^[ \t]*protected[ \t]+def[ \t]*([a-zA-Z0-9_=]+)[ \t]*.*[:=]/\1/m,methods/
--regex-scala=/^[ \t]*private[ \t]+def[ \t]*([a-zA-Z0-9_=]+)[ \t]*.*[:=]/\1/m,methods/
--regex-scala=/^[ \t]*val[ \t]*([a-zA-Z0-9_=]+)[ \t]*.*[:=]/\1/m,methods/
--regex-scala=/^[ \t]*protected[ \t]+val[ \t]*([a-zA-Z0-9_=]+)[ \t]*.*[:=]/\1/m,methods/
--regex-scala=/^[ \t]*private[ \t]+val[ \t]*([a-zA-Z0-9_=]+)[ \t]*.*[:=]/\1/m,methods/
--regex-scala=/^[ \t]*type[ \t]*([a-zA-Z0-9_]+)[ \t]*[\[<>=]/\1/T,types/
--exclude=.git
--exclude=conf
--exclude=dist
--exclude=target
--exclude=lib_managed
--exclude=src_managed
--exclude=project
--exclude=public
--exclude=assets
--exclude=*.js
--exclude=*.css
--exclude=*.java
--exclude=Play21
